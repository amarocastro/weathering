		<NavigationView ItemInvoked="NavigationView_OnItemInvoked"
						IsBackButtonVisible="Collapsed">
			<NavigationView.AutoSuggestBox>
				<AutoSuggestBox
								QueryIcon="Find" Name="SearchPlace" 
								PlaceholderText="Search Your Place"
								TextChanged="AutoSuggestBox_TextChanged"
								QuerySubmitted="AutoSuggestBox_QuerySubmitted"
								SuggestionChosen="AutoSuggestBox_SuggestionChosen">
					<AutoSuggestBox.ItemTemplate>
						<DataTemplate>
							<Grid Padding="0,12,0,12" Tag="{Binding id}">
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto"/>
									<RowDefinition Height="*"/>
								</Grid.RowDefinitions>
								<StackPanel  Grid.Row="0" Orientation="Horizontal">
									<TextBlock Text="{Binding title}" Width="248"/>
									<FontIcon Glyph="&#xE707;" HorizontalAlignment="Right" VerticalAlignment="Center"/>
								</StackPanel>
								<StackPanel Grid.Row="1" Orientation="Horizontal">
									<TextBlock Text="{Binding subtitle}" FontSize="12"/>
								</StackPanel>
							</Grid>
						</DataTemplate>
					</AutoSuggestBox.ItemTemplate>
				</AutoSuggestBox>
			</NavigationView.AutoSuggestBox>
			<NavigationView.MenuItems>
				<!--<NavigationViewItem Icon="Find" Content="Search" Tag="SearchPage"/>-->
				<NavigationViewItem Icon="MapPin" Content="Plan" Tag="LocationPage"/>
				<NavigationViewItem Icon="List" Content="Favourites" Tag="FavouritePage"/>
			</NavigationView.MenuItems>
			<Frame x:Name="ContentFrame" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
					Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">>

				<Frame.ContentTransitions>
					<TransitionCollection>
						<NavigationThemeTransition/>
					</TransitionCollection>
				</Frame.ContentTransitions>
			</Frame>
		</NavigationView>
